{
   "name": "typeorm-sample",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/body-parser": "^1.19.2",
      "@types/express": "^4.17.14",
      "@types/jsonwebtoken": "^8.5.9",
      "@types/node": "^16.11.10",
      "nodemon": "^2.0.20",
      "ts-node": "^10.9.1",
      "typescript": "^4.8.3"
   },
   "dependencies": {
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.20.0",
      "dotenv": "^16.0.2",
      "express": "^4.18.1",
      "jsonwebtoken": "^8.5.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.9"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs -d ./src/database/data_source.ts",
      "migration:create": "typeorm migration:create src/database/migrations/CreateUsersTables",
      "migration:run": "typeorm-ts-node-commonjs -d ./src/database/data_source.ts migration:run",
      "dev": "nodemon src/index.ts"
   }
}
